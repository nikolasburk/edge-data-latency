generator client {
  provider = "prisma-client-js"
  output   = "./prisma-results"
}

datasource db {
  provider = "postgresql"
  url      = env("RESULTS_DATABASE_URL")
}

enum DataService {
  Neon
  PrismaNeon
}

enum Runtime {
  Edge
  Node
}

enum QueryCount {
  One
  Two
  Five
}

enum Location {
  Regional
  Global
}

model BenchmarkRun {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())

  // Benhcmark run params
  dataService DataService
  runtime     Runtime
  location    Location
  queryCount  QueryCount

  // Meta
  route String

  // Result
  queryDuration Int
}
